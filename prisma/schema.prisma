// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Produto {
  id Int

  name String
  category String
  salesUnit String
  purchasePrice String
  salePrice String
  observations String

  createdAt DateTime
  updateAt DateTime

}
 
 