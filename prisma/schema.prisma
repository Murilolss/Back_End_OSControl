// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User{
  id Int

  email String
  senha String
  cpf String
  phone String
  birth Date
  signature Float
  isActive Boolean

  createdAt DateTime
  updateAt DateTime

}

model Client {
  id String

  name String
  cpf String
  phone String
  email String
  address String
  number Int
  neighborhood String
  state String
  city String

  createdAt DateTime
  updateAt DateTime
  
}